server
{
    
	server_name=www.exemple.com

	listen=127.0.0.1:8080

	client_body_size_max=1844674407370955161556465498498

	error_page=404 /error_page.html

	location / 
    {

		allow_methods=GET	POST

		# redirect=303 /redirect.html

		root=/Users/ydahni/Desktop/webserv/root
		
		autoindex=on

		default=/default.html

		cgi=.py /bin/python3

		upload=/upload

	}

	location /a 
    {

		allow_methods=GET POST DELETE

		redirect=301 /redirect.html

		root=/path/to/dir
		
		autoindex=on

		default=/default.html

		cgi=.py /bin/python3

		upload=/goinfre

	}

}

server
{
    
	server_name=test2

	listen=127.0.0.1:8081

	client_body_size_max=99999999999
	error_page=404 /error_page.html
	error_page=405 /maerror_page.html


	location / 
    {

		allow_methods=GET	POST

		# redirect=303 /redirect.html

		root=/Users/ydahni/Desktop/webserv/root
		
		autoindex=on

		default=/default.html

		cgi=.py /bin/python3

		upload=/upload

	}

	location /a 
    {

		allow_methods=GET POST DELETE

		redirect=301 /redirect.html

		root=/path/to/dir
		
		autoindex=on

		default=/default.html

		cgi=.php /bin/php

		upload=/goinfre

	}


}



